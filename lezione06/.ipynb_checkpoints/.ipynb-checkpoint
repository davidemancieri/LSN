   "source": [
    "heat=((beta*J)**2)*(((1+thN+(Ns-1)*(th**2)+(Ns-1)*(ch**2)*thN)/(1+thN))-Ns*((th+ch*thN)/(1+thN))**2)\n",
    "plt.plot(T, heat, label=\"Teorica\")\n",
    "plt.title('Ising 1D, Heat capacity')\n",
    "x, sum_prog, err_prog = np.loadtxt( \"data/heatMetro.dat\", usecols=(0,1,2), delimiter=' ', unpack='true' )\n",
    "plt.errorbar(x,sum_prog,yerr=err_prog, label=\"Metropolis\")\n",
    "x, sum_prog, err_prog = np.loadtxt( \"data/heatGibbs.dat\", usecols=(0,1,2), delimiter=' ', unpack='true' )\n",
    "plt.errorbar(x,sum_prog,yerr=err_prog, label=\"Gibbs\")\n",
    "plt.xlabel('T')\n",
    "plt.ylabel('C')\n",
    "plt.grid()\n",
    "plt.legend()\n",
    "plt.show()"
