   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "points=100\n",
    "T = np.linspace(0.5,2.0,num=points)\n",
    "beta = 1/T\n",
    "J = 1.0\n",
    "Ns = 50\n",
    "th = np.tanh(J/T)\n",
    "thN= th**Ns\n",
    "ch = 1/th\n",
    "e = -J*( th + ch*thN )/( 1 + thN )\n",
    "plt.plot(T, e, label=\"Teorica\")\n",
    "x, sum_prog, err_prog = np.loadtxt( \"data/eneMetro.dat\", usecols=(0,1,2), delimiter=' ', unpack='true' )\n",
    "plt.errorbar(x,sum_prog,yerr=err_prog, label=\"Metropolis\")\n",
    "x, sum_prog, err_prog = np.loadtxt( \"data/eneGibbs.dat\", usecols=(0,1,2), delimiter=' ', unpack='true' )\n",
    "plt.errorbar(x,sum_prog,yerr=err_prog, label=\"Gibbs\")\n",
    "plt.title('Ising 1D, internal energy')\n",
    "plt.xlabel('T')\n",
    "plt.ylabel('U/N')\n",
    "plt.grid()\n",
    "plt.legend()\n",
    "plt.show()"
   ]
  },
